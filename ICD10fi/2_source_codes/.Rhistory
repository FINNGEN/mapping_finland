range_to_regex <- function(match){
range <- str_extract_all(match, "[:digit:][:digit:]") %>% unlist() %>% as.integer()
match <- range[1]:range[2] %>% as.character() %>% if_else(nchar(.)==1, str_c("0",.),.) %>% str_c(collapse = "|")
return(str_c("(",match,")"))
}
